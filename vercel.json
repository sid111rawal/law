{
  "redirects": [
    {
      "source": "/contact.html",
      "destination": "/contact/",
      "permanent": true
    },
    {
      "source": "/about.html",
      "destination": "/about/",
      "permanent": true
    },
    {
      "source": "/tools.html",
      "destination": "/resources/calculators/",
      "permanent": true
    },
    {
      "source": "/tools/:calculator",
      "destination": "/resources/calculators/:calculator",
      "permanent": true
    },
    {
      "source": "/tools/:calculator/",
      "destination": "/resources/calculators/:calculator/",
      "permanent": true
    },
    {
      "source": "/services.html",
      "destination": "/services/",
      "permanent": true
    },
    {
      "source": "/services/:service.html",
      "destination": "/services/:service/",
      "permanent": true
    },
    {
      "source": "/resources/calculators/:calculator.html",
      "destination": "/resources/calculators/:calculator/",
      "permanent": true
    },
    {
      "source": "/resources/blogs.html",
      "destination": "/resources/blogs/",
      "permanent": true
    },
    {
      "source": "/blogs.php",
      "destination": "/resources/blogs/",
      "permanent": true
    },
    {
      "source": "/blogs.php",
      "destination": "/resources/blogs/category/compliance",
      "permanent": true,
      "has": [
        {
          "type": "query",
          "key": "category",
          "value": "compliance"
        }
      ]
    },
    {
      "source": "/blogs.php",
      "destination": "/resources/blogs/category/tax-planning",
      "permanent": true,
      "has": [
        {
          "type": "query",
          "key": "category",
          "value": "tax-planning"
        }
      ]
    },
    {
      "source": "/blogs.php",
      "destination": "/resources/blogs/category/business-formation",
      "permanent": true,
      "has": [
        {
          "type": "query",
          "key": "category",
          "value": "business-formation"
        }
      ]
    },
    {
      "source": "/resources/blogs/:slug.html",
      "destination": "/resources/blogs/:slug/",
      "permanent": true
    },
    {
      "source": "/privacy-policy.html",
      "destination": "/privacy-policy/",
      "permanent": true
    },
    {
      "source": "/terms-and-conditions.html",
      "destination": "/terms-and-conditions/",
      "permanent": true
    },
    {
      "source": "/cookie-policy.html",
      "destination": "/cookie-policy/",
      "permanent": true
    },
    {
      "source": "/:path*.html",
      "destination": "/:path*/",
      "permanent": true
    }
  ]
}

